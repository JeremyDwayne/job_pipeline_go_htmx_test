package templates

templ Index(email string) {
	<div class="container flex flex-col items-center justify-center gap-12 px-4 py-16">
		<h1 class="text-5xl font-extrabold tracking-tight sm:text-[5rem]">
			Welcome { email }
		</h1>
	</div>
}

